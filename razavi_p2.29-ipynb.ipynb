   "source": [
    "from numpy import exp\n",
    "from scipy.optimize import fsolve\n",
    "\n",
    "Is = 5e-16\n",
    "VT = 0.026\n",
    "\n",
    "Ix = 2e-3\n",
    "\n",
    "def f29(Vr):\n",
    "    # Ix equation from KCL\n",
    "    return (Vr / 2e3) + Is * (exp(Vr/(2*VT)) - 1) - Ix\n",
    "\n",
    "fsolve(f29, 1.5)"
   ]
  }
